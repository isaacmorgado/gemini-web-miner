{
  "mcpServers": {
    "gemini-web-miner": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--volume",
        "/tmp/crawl4ai-crawls:/app/crawls",
        "uysalsadi/crawl4ai-mcp-server:latest"
      ],
      "env": {
        "CRAWL4AI_MCP_LOG": "INFO",
        "GEMINI_API_KEY": "${env:GEMINI_API_KEY}"
      },
      "alwaysAllow": ["crawl", "scrape"],
      "disabled": false
    }
  }
}
